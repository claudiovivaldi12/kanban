Task.vue","name":null,"original":{"line":39,"column":0},"generated":{"line":89,"column":0}}],"sources":{"src/components/AddTask.vue":"<template>\r\n  <div id=\"test-add\">\r\n    <form>\r\n      <h3>Add New Todo</h3>\r\n      <table>\r\n        <tr>\r\n          <td><label for=\"title\">Title</label></td>\r\n          <td><input type=\"text\" v-model=\"task.title\" placeholder=\"Title\"></td>\r\n        </tr>\r\n        <tr>\r\n          <td><label for=\"description\">description</label></td>\r\n          <td><input  type=\"text\" v-model=\"task.description\" placeholder=\"Description\"></td>\r\n        </tr>\r\n        <tr>\r\n          <td><label for=\"category\">Category</label></td>\r\n          <td>\r\n          <select v-model=\"task.category\">\r\n            <option value=\"Backlog\">Backlog</option>\r\n            <option value=\"Progress\">Progress</option>\r\n            <option value=\"Strugle\">Strugle</option>\r\n            <option value=\"Done\">Done</option>\r\n          </select>\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>Date</td>\r\n          <td><input type=\"date\" v-model=\"task.due_date\"><br></td>\r\n        </tr>\r\n        <tr>\r\n          <td><button @click=\"saveTask\">Save</button></td>\r\n          <td><button @click=\"kembali\">Back</button></td>\r\n        </tr>\r\n      </table>\r\n  </form>\r\n  </div>\r\n</template>\r\n<script>\r\n import axios from'axios'\r\n  export default{\r\n    name:'AddTask',\r\n    data(){\r\n      return{\r\n        task:{\r\n          title:'',\r\n          description:'',\r\n          category:'',\r\n          due_date:''\r\n        }\r\n      }\r\n    },\r\n    methods:{\r\n      kembali(){\r\n        this.$emit(\"kembali\")\r\n      },\r\n      saveTask(){\r\n        axios({\r\n          method:'Post',\r\n          url:'https://kanban-devp.herokuapp.com/',\r\n          data:{\r\n            title:this.task.title,\r\n            description:this.task.description,\r\n            category:this.task.category,\r\n            due_date:this.task.due_date\r\n          },\r\n          headers:{\r\n            token:localStorage.getItem('access_token')\r\n          }\r\n        })\r\n        .then(data =>{\r\n          console.log(data)\r\n          console.log('Success')\r\n          this.$emit(\"backToHome\")\r\n        })\r\n        .catch(err =>{\r\n          console.log(err)\r\n        })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n"},"lineCount":null}},"error":null,"hash":"2324fce6ac1e484719d304d8dbf2dbb8","cacheData":{"env":{}}}